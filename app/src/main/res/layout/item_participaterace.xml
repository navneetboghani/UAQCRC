<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/colorPrimaryDark">

    <TableLayout
        android:id="@+id/simpleTableLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="16dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:weightSum="5">

            <TableLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4.5">

                <TableRow
                    android:id="@+id/firstRow"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content">
                    <!-- first element of the row-->
                    <TextView
                        android:id="@+id/txt_roundname"
                        android:layout_width="90dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/topleftcell"
                        android:fontFamily="sans-serif-medium"
                        android:gravity="right"
                        android:padding="@dimen/_5dp"
                        android:text="@string/round_name"
                        android:textColor="@color/lighttext"
                        android:textSize="@dimen/_15sp" />

                    <TextView
                        android:id="@+id/tv_roundname"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="-1dp"
                        android:layout_weight="1"
                        android:background="@drawable/cell_shape"
                        android:gravity="left"
                        android:padding="@dimen/_5dp"
                        android:text="round_name"
                        android:textColor="@color/red"
                        android:textSize="15sp"
                        android:textStyle="bold" />
                </TableRow>

                <TableRow
                    android:id="@+id/secondRow"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content">
                    <!-- first element of the row-->
                    <TextView
                        android:id="@+id/txt_type"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="-1dp"
                        android:background="@drawable/cell_shape"
                        android:fontFamily="sans-serif-medium"
                        android:gravity="right"
                        android:padding="@dimen/_5dp"
                        android:text="@string/type"
                        android:textColor="@color/lighttext"
                        android:textSize="@dimen/_15sp" />

                    <TextView
                        android:id="@+id/tv_type"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="-1dp"
                        android:layout_marginTop="-1dp"
                        android:layout_weight="1"
                        android:background="@drawable/cell_shape"
                        android:gravity="left"
                        android:padding="@dimen/_5dp"
                        android:text="type"
                        android:textColor="@color/black"
                        android:textSize="15sp" />
                </TableRow>

                <TableRow
                    android:id="@+id/thirdRow"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content">
                    <!-- first element of the row-->
                    <TextView
                        android:id="@+id/txt_distance"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="-1dp"
                        android:background="@drawable/cell_shape"
                        android:fontFamily="sans-serif-medium"
                        android:gravity="right"
                        android:padding="@dimen/_5dp"
                        android:text="@string/distance"
                        android:textColor="@color/lighttext"
                        android:textSize="@dimen/_15sp" />

                    <TextView
                        android:id="@+id/tv_distance"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="-1dp"
                        android:layout_marginTop="-1dp"
                        android:layout_weight="1"
                        android:background="@drawable/cell_shape"
                        android:gravity="left"
                        android:padding="@dimen/_5dp"
                        android:text="distance"
                        android:textColor="@color/black"
                        android:textSize="15sp" />
                </TableRow>

                <TableRow
                    android:id="@+id/fourthRow"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content">
                    <!-- first element of the row-->
                    <TextView
                        android:id="@+id/txt_price"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="-1dp"
                        android:background="@drawable/cell_shape"
                        android:fontFamily="sans-serif-medium"
                        android:gravity="right"
                        android:padding="@dimen/_5dp"
                        android:text="@string/prize"
                        android:textColor="@color/lighttext"
                        android:textSize="@dimen/_15sp" />

                    <TextView
                        android:id="@+id/tv_price"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="-1dp"
                        android:layout_marginTop="-1dp"
                        android:layout_weight="1"
                        android:background="@drawable/cell_shape"
                        android:gravity="left"
                        android:padding="@dimen/_5dp"
                        android:text="price"
                        android:textColor="@color/black"
                        android:textSize="15sp" />
                </TableRow>

                <TableRow
                    android:id="@+id/fifthhRow"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content">
                    <!-- first element of the row-->
                    <TextView
                        android:id="@+id/txt_category"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="-1dp"
                        android:background="@drawable/bottomleftcell"
                        android:fontFamily="sans-serif-medium"
                        android:gravity="right"
                        android:padding="@dimen/_5dp"
                        android:text="@string/category"
                        android:textColor="@color/lighttext"
                        android:textSize="@dimen/_15sp" />

                    <TextView
                        android:id="@+id/tv_category"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="-1dp"
                        android:layout_marginTop="-1dp"
                        android:layout_weight="1"
                        android:background="@drawable/cell_shape"
                        android:gravity="left"
                        android:padding="@dimen/_5dp"
                        android:text="category"
                        android:textColor="@color/black"
                        android:textSize="15sp" />
                </TableRow>
            </TableLayout>

            <TextView
                android:id="@+id/tv_noofparticipant1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:background="@drawable/toprightandbottomright"
                android:gravity="center"
                android:text="0"
                android:textColor="@color/red"
                android:textSize="18sp"
                android:textStyle="bold" />
        </TableRow>
    </TableLayout>
